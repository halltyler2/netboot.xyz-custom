#!ipxe
###################
### custom menu
###################
:start
chain --autofree boot.cfg ||

:custom
clear custom_choice
menu Menu
item --gap
item hbcd ${space} Hirens Boot CD

choose custom_choice || goto custom_exit
goto ${custom_choice}

:hbcd
initrd http://192.168.1.4:80/HirensBootCD/HBCD_PE_x64.iso
chain memdisk iso raw ||

:custom_exit
exit
